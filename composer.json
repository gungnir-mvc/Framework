{
    "name": "gungnir-mvc/framework",
    "description": "MVC Framework library for gungnir-mvc",
    "type": "library",
    "require": {
        "gungnir-mvc/core": "dev-master",
        "gungnir-mvc/http": "1.*",
        "gungnir-mvc/event": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "5.5.*",
        "phpspec/prophecy": "~1.0",
        "mikey179/vfsStream": "~1.6"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/gungnir-mvc/core.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/gungnir-mvc/http.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/gungnir-mvc/event.git"
        }
    ],
    "autoload": {
        "psr-4": {
            "Gungnir\\Framework\\": "src/"
        },
        "exclude-from-classmap": [
            "/tests/"
        ]
    }
}
